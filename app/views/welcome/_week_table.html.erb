

<div class="week" summary="Users for week">
  <div class="date">
        <%= event.date.day.to_s + " " + event.date.strftime("%B") %>
  </div>
  <ul>
    <% event.users.each do |user| %>
    <li >
        <% if user == current_user %>
          <a class="user_name" href=<%="welcome/remove_from_month?event_id="+event.id.to_s%>>
            <%= user.first_name %>
          </a>
        <% else %>
          <span class="user_name">
            <%= user.first_name %>
          </span>
        <%end %>

    <% end %>

    <% if !event.users.include?(current_user) %>
    <li>
        <a class="signUpRowButton" href=<%="welcome/sign_up_month?event_id="+event.id.to_s%>> <%= "+" + " " + current_user.first_name%></a>

    </li>
    <% end %>
  </ul>
</div>
  